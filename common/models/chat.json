{
  "name": "Chat",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "name": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
    "chatMembers": {
      "type": "hasMany",
      "model": "Person",
      "foreignKey": "chatId",
      "through": "ChatMember"
    },
    "chatOwner": {
      "type": "belongsTo",
      "model": "Person",
      "foreignKey": "ownerId"
    },
    "chatMessages": {
      "type": "hasMany",
      "model": "Message",
      "foreignKey": "chatId"
    }
  },
  "acls": [],
  "methods": {}
}
